<?eclipse version="3.4"?>
<plugin>

   <extension
         point="com.ibm.cics.core.comm.connections">
      <connection
            category="com.ibm.cics.zos.comm.connection"
            class="de.tgmz.zdev.zowe.connection.ZoweConnection"
            id="de.tgmz.zdev.zowe.connection"
            name="%label.zowe"
            ssl_enabled="true"
            certificate_auth="true">
      </connection>
   </extension>
   
   <extension 
        point="com.ibm.cics.core.connections.connectable">
        <connectable 
            categoryId="de.tgmz.zdev.zowe.connection"
            class="com.ibm.cics.zos.model.ZOSConnectable">
        </connectable>
   </extension>
   
</plugin>
